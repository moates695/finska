{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2015", "DOM"],
    "target": "ES2015",
    "skipLibCheck": true,
    "isolatedModules": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "tests"
  ]
}
